{
  "name": "free-crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "vitest run tests/accessibility",
    "test:storybook": "vitest run --config vitest.storybook.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "audit:security": "npm audit",
    "audit:outdated": "npm outdated",
    "audit:license": "npx license-checker --production --onlyAllow \"MIT;Apache-2.0;ISC;BSD-2-Clause;BSD-3-Clause;CC0-1.0;0BSD;Unlicense;WTFPL;MPL-2.0\" --summary",
    "audit:full": "node scripts/audit-dependencies.js",
    "audit:report": "node scripts/audit-dependencies.js --markdown",
    "audit:sbom": "npx @cyclonedx/cyclonedx-npm --output-format json --output-file sbom.json",
    "sonar:fetch": "node scripts/sonar/fetch-issues.js",
    "sonar:sync": "node scripts/sonar/sync-to-notion.js",
    "sonar:generate": "node scripts/sonar/generate-fixes.js",
    "sonar:all": "npm run sonar:fetch && npm run sonar:generate && npm run sonar:sync",
    "notion:sync": "node scripts/sync-to-notion.js",
    "notion:dev": "node scripts/sync-to-notion.js dev",
    "notion:test": "node scripts/sync-to-notion.js test",
    "notion:quality": "node scripts/sync-to-notion.js quality",
    "notion:meeting": "node scripts/sync-to-notion.js meeting"
  },
